  }else if( 8 <= num && num <= 10){
      icon ='marker_orange.png';
  }else if( 11 <= num && num <= 15){
      icon ='marker_red.png';
  }else if( 16 <= num && num <= 20){
      icon ='marker_gray.png';
  }else if( 21 <= num ){
